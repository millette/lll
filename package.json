{
  "name": "lll",
  "version": "0.0.0",
  "main": "index.js",
  "bin": "server.js",
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "deps": "dependency-check . && dependency-check . --unused --no-dev",
    "docs": "documentation build index.js -f md -o docs.md && prettier --write docs.md",
    "start": "node server.js",
    "test": "healthier && nyc ava"
  },
  "author": {
    "name": "Robin Millette",
    "email": "robin@millette.info",
    "url": "http://robin.millette.info"
  },
  "engines": {
    "node": ">= 10.9.0"
  },
  "license": "AGPL-3.0",
  "dependencies": {
    "ajv": "^6.10.2",
    "ajv-i18n": "^3.4.0",
    "encoding-down": "^6.1.0",
    "env-schema": "^1.0.0",
    "fastify": "^2.6.0",
    "fastify-cookie": "^3.0.2",
    "fastify-formbody": "^3.1.0",
    "fastify-session": "^3.0.0",
    "level-errors": "^2.0.1",
    "leveldown": "^5.1.1",
    "levelup": "^4.1.0",
    "make-dir": "^3.0.0",
    "memorystore": "^1.6.1",
    "through2": "^3.0.1"
  },
  "devDependencies": {
    "ava": "^2.2.0",
    "coveralls": "^3.0.5",
    "del": "^5.0.0",
    "dependency-check": "^3.4.1",
    "documentation": "^12.0.1",
    "healthier": "^2.0.0",
    "husky": "2.7.0",
    "lint-staged": "^9.2.0",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2"
  },
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "prettier": {
    "semi": false,
    "trailingComma": "es5",
    "arrowParens": "always"
  }
}
