{
  "name": "lll",
  "version": "0.0.0",
  "main": "index.js",
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "deps": "dependency-check . && dependency-check . --unused --no-dev",
    "docs": "documentation build index.js -f md -o docs.md && prettier --write docs.md",
    "test": "healthier && nyc ava"
  },
  "author": {
    "name": "Robin Millette",
    "email": "robin@millette.info",
    "url": "http://robin.millette.info"
  },
  "engines": {
    "node": ">= 10.9.0"
  },
  "license": "AGPL-3.0",
  "dependencies": {
    "ajv": "^6.10.0",
    "ajv-i18n": "^3.4.0",
    "encoding-down": "^6.0.1",
    "level-errors": "^2.0.0",
    "leveldown": "^4.0.2",
    "levelup": "^4.0.0",
    "make-dir": "^2.1.0",
    "through2": "^3.0.1"
  },
  "devDependencies": {
    "ava": "^1.4.0",
    "coveralls": "^3.0.3",
    "del": "^4.0.0",
    "dependency-check": "^3.3.0",
    "documentation": "^9.3.1",
    "healthier": "^2.0.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "nyc": "^13.3.0",
    "prettier": "^1.16.4"
  },
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "prettier": {
    "semi": false,
    "trailingComma": "es5",
    "arrowParens": "always"
  }
}
